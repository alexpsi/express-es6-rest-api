{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/index.js\"",
    "build": "babel src -s -D -d dist",
    "prod": "node dist/index.js",
    "start": "node dist/index.js",
    "prestart": "yarn run -s build",
    "lint": "eslint src",
    "test": "jest src"
  },
  "repository": "alexpsi/express-es6-rest-api",
  "author": "Alex Psi <alexpsi256@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "nodemon": "^1.9.2",
    "supertest": "^3.0.0"
  }
}
